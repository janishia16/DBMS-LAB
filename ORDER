mysql> CREATE VIEW HIGHEST_AMT(SALESMAN_ID,NAME,CITY) AS SELECT SALESMAN.SALESMAN_ID,NAME,SALESMAN.CITY FROM SALESMAN,CUSTOMER WHERE CUSTOMER.SALESMAN_ID=SALESMAN.SALESMAN_ID AND CUSTOMER_ID IN (SELECT CUSTOMER_ID FROM ORDERS GROUP BY ORDER_DATE,CUSTOMER_ID HAVING SUM(PURCHASE_AMT)=(SELECT MAX(PRD_AMT) FROM (SELECT CUSTOMER_ID,SUM(PURCHASE_AMT) PRD_AMT,ORDER_DATE FROM ORDERS GROUP BY CUSTOMER_ID, ORDER_DATE) AS PRD_AMT));
